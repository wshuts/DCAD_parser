<Window x:Class="DCAD_parser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel Name="dockPanelMainWindow">
            <StatusBar x:Name="ProgressStatusBar" DockPanel.Dock="Bottom" Height="23">
                <StatusBarItem x:Name="ProgressItem">
                    <TextBlock x:Name="ProgressText">Hello</TextBlock>
                </StatusBarItem>
            </StatusBar>
            <Button Content="Launch" Height="23" Name="buttonLaunchManager" Width="75" Click="buttonLaunchManager_Click" />
        </DockPanel>
    </Grid>
</Window>
